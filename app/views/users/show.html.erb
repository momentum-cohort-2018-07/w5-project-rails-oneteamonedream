<h1><%= @user.name %>'s Books!</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Username</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
  <% @user.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
    </tr>
  <% end %>
  </tbody>
</table>



<%= link_to 'Back', root_path %>